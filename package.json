{
  "name": "style-skeleton-boilerplate",
  "version": "0.0.0",
  "description": "Example style modules",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --open --config webpack.dev.js",
    "dist": "webpack --config webpack.prod.js",
    "test:ci": "NODE_ENV=test jest test",
    "test": "NODE_ENV=test jest test --notify --watch",
    "cleancoverage": "rm -rf coverage",
    "prerelease": "npm ci && npm run lint && npm run cleancoverage && npm run check-master && npm run test:ci",
    "clean": "rm -rf dist && mkdir dist",
    "predist": "npm run clean",
    "lint": "eslint .",
    "check-master": "git symbolic-ref --short HEAD | grep ^master$ > /dev/null || (echo \"Not in 'master' branch. Aborting release\"; return 1)",
    "release:patch": "npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch",
    "release:minor": "npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor",
    "release:major": "npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major",
    "prepare": "npm run dist",
    "postversion": "git push --follow-tags",
    "deploy": "webpack --config webpack.deploy.js && surge ./deploy/builds/ [nombre componente eg:'roi-tabs'].surge.sh",
    "style-lint": "node_modules/sass-lint/bin/sass-lint.js --config .scss-lint.yml '**/*.scss' --verbose --no-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pableco/style-skeleton-boilerplate.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/pableco/style-skeleton-boilerplate",
  "engines": {
    "node": ">=6.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^4.3.2",
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^2.1.1",
    "css-selector-tokenizer": "^0.7.1",
    "cssnano": "^4.1.10",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^5.2.2",
    "generate-changelog": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jest": "^23.6.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-smart-import": "^0.7.6",
    "react": "^16.8.6",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "surge": "^0.21.3",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dashboard": "^3.0.7",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.10"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "dependencies": {}
}
