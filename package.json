{
  "name": "style-skeleton-boilerplate",
  "version": "0.0.0",
  "description": "Example style modules",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dashboard -- webpack-dev-server --config webpack.config.dev.js",
    "test:watch": "mocha -r test/registerBabel.js -r ignore-styles --watch --recursive --growl",
    "test": "istanbul cover --report text --report lcov _mocha -- -r test/registerBabel.js -r ignore-styles --recursive --growl",
    "clean": "rm -rf dist && mkdir dist",
    "predist": "npm run clean",
    "dist": "webpack --config webpack.config.dist.js",
    "lint": "eslint .",
    "check-coverage": "istanbul check-coverage --statements 50 --branches 50 --functions 70",
    "prepublish": "npm run dist",
    "check-master": "git symbolic-ref --short HEAD | grep ^master$ > /dev/null || (echo \"Not in 'master' branch. Aborting release\"; return 1)",
    "prerelease": "npm run lint && npm run check-master && npm run test && npm run check-coverage",
    "release:patch": "npm run prerelease && changelog -p && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version patch && git push --follow-tags",
    "release:minor": "npm run prerelease && changelog -m && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version minor && git push --follow-tags",
    "release:major": "npm run prerelease && changelog -M && git add CHANGELOG.md && git commit -m 'updated CHANGELOG.md' && npm version major && git push --follow-tags",
    "postversion": "git push --follow-tags",
    "style-lint": "node_modules/sass-lint/bin/sass-lint.js --config .scss-lint.yml '**/*.scss' --verbose --no-exit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pableco/style-skeleton-boilerplate.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/pableco/style-skeleton-boilerplate",
  "engines": {
    "node": ">=6.2"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.11",
    "css-selector-tokenizer": "^0.7.1",
    "duplicate-package-checker-webpack-plugin": "^1.2.6",
    "enzyme": "^2.4.1",
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.0",
    "eslint-plugin-import": "^1.10.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "generate-changelog": "^1.7.1",
    "growl": "^1.10.5",
    "ignore-styles": "^5.0.1",
    "istanbul": "1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "node-sass": "^4.12.0",
    "pixrem": "^3.0.2",
    "postcss-loader": "^1.0.0",
    "postcss-smart-import": "^0.7.6",
    "react": "^15.6.2",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^15.6.2",
    "sass-lint": "^1.13.1",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "webpack": "^1.15.0",
    "webpack-cli": "^3.3.4",
    "webpack-dashboard": "^2.1.0",
    "webpack-dev-server": "^3.7.1",
    "webpack-visualizer-plugin": "^0.1.10"
  },
  "peerDependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
